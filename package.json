{
	"name": "obsidian-archetype",
	"version": "0.1.0",
	"description": "Speed reading and active learning through timed text segments and touch typing progression.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage"
	},
	"keywords": ["obsidian", "plugin", "speed-reading", "active-learning", "touch-typing"],
	"author": "Anthony Brunyee",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.19.26",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"@vitest/ui": "^4.0.15",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"happy-dom": "^20.0.11",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vitest": "^4.0.15"
	}
}
